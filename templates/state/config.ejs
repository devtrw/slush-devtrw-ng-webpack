import template     from './<%= baseState %>.html';
import translations from './<%= baseState %>-translations' %>;

function <%= stateConfigFn %>($stateProvider, $translateProvider) {

  $translateProvider.translations(
    'en',
    <%= translationsPath[0] %> translations <%= translationsPath[1] %>
  );

  $stateProvider.state('<%= state %>', {
    url:        '/<%= baseState %>',
    template:   template,
    controller: '<%= stateCtrlFn %> as ctrl'
  });
}

export default /*@ngInject*/ <%= stateConfigFn %>;
